@model IEnumerable<BMEDmgt.Areas.MedEngMgt.Models.RepKeepStokVModel>

@*<p>
    @Html.ActionLink("匯出Excel", "RepKeepStokExcel")
</p>*@
<table class="table">
    <tr>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.CustId)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.CustNam)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.KeepAmt)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.RepairAmt)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.TotalAmt)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.KeepCost)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.RepairCost)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.TotalCost)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.CustId)
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.CustNam)
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.KeepAmt)
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.RepairAmt)
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.TotalAmt)
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.KeepCost)
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.RepairCost)
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.TotalCost)
            </td>
        </tr>
    }

</table>
