@model IEnumerable<BMEDmgt.Areas.MedEngMgt.Models.ProperRate>

<h4>設備妥善率統計表</h4>
<p>
    <a class="btn btn-default" href="@Url.Action("ExcelAssetProperRate", TempData["qry"])">匯出Excel</a>
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AssetNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssetName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Brand)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccDptNam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RepairDays)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RepairCnts)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssetProperRate)
        </th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AssetNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssetName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Brand)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccDptNam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RepairDays)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RepairCnts)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssetProperRate)
        </td>
    </tr>
    }
</table>
