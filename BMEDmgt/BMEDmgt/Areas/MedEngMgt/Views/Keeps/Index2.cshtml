<script src="~/Areas/MedEngMgt/Scripts/QryDpt.js"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
<h4>保養案件查詢</h4>
@using (Ajax.BeginForm("Index2", "Keeps", null,
                new AjaxOptions
                {
                    UpdateTargetId = "pnlKEEPLIST",
                    HttpMethod = "POST",
                    LoadingElementId = "imgLOADING2",
                    OnSuccess = "showmsg"
                },
                new { @class = "form-horizontal", id = "frmKEEP" }))
{
    <div class="form-group">
        <div class="form-inline">
            @Html.Label("qtyDOCID", "表單編號", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.TextBox("qtyDOCID", "", new { @class = "form-control" })
            </div>
            @Html.Label("qtyASSETNO", "財產編號", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.TextBox("qtyASSETNO", "", new { @class = "form-control" })
            </div>
            @Html.Label("qtyASSETNAME", "儀器名稱(關鍵字)", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.TextBox("qtyASSETNAME", "", new { @class = "form-control" })
            </div>
        </div>
    </div>
    <div class="form-group">
        @Html.Label("qtyTYPE", "型號", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10 text-left">
            <div class="form-inline">
                @Html.TextBox("qtyTYPE", "", new { @class = "form-control" })
            </div>
        </div>
    </div>
    <div class="form-group">
        @Html.Label("AccdptKeyName", "成本中心", new { @class = "control-label col-md-2" })
        <div class="col-md-10 text-left">
            <div class="form-inline">
                @Html.TextBox("AccdptKeyName", "", new { @class = "form-control", placeholder = "代號或關鍵字" })
                <input id="btnQtyAccdpt" type="button" value="查詢" class="btn btn-default">
                @Html.DropDownList("qtyACCDPT", ViewData["ACCDPT"] as SelectList, "請選擇", new { @class = "form-control" })
            </div>
        </div>
    </div>
    <div class="form-group">
        @Html.Label("ApplydptKeyName", "申請部門", new { @class = "control-label col-md-2" })
        <div class="col-md-10 text-left">
            <div class="form-inline">
                @Html.TextBox("ApplydptKeyName", "", new { @class = "form-control", placeholder = "代號或關鍵字" })
                <input id="btnQtyApplydpt" type="button" value="查詢" class="btn btn-default">
                @Html.DropDownList("qtyDPTID", ViewData["APPLYDPT"] as SelectList, "請選擇", new { @class = "form-control" })
            </div>
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Sdate", "出單日期", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10 text-left">
            <div class="form-inline">
                @Html.Editor("Sdate", "DateTime", new { htmlAttributes = new { @class = "form-control" } })
                @Html.Label("Edate", "~", htmlAttributes: new { @class = "control-label" })
                @Html.Editor("Edate", "DateTime", new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="form-inline">
            @Html.Label("qtyINOUT", "保養方式", new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.DropDownList("qtyINOUT", ViewData["INOUT"] as SelectList, "請選擇", new { @class = "form-control" })
            </div>
            @Html.Label("qtyFLOWTYP", "流程狀態", new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.DropDownList("qtyFLOWTYP", ViewData["FLOWTYP"] as SelectList, null, new { @class = "form-control" })
            </div>
        </div>
    </div>
    <div class="form-group">
        @Html.Label("FlowKeyName", "目前關卡人員", new { @class = "control-label col-md-2" })
        <div class="col-md-10 text-left">
            <div class="form-inline">
                @Html.TextBox("FlowKeyName", "", new { @class = "form-control", placeholder = "代號或關鍵字" })
                <input id="btnQtyFlow" type="button" value="查詢" class="btn btn-default">
                @Html.DropDownList("qtyFLOW", ViewData["FLOWID"] as SelectList, "請選擇", new { @class = "form-control" })
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input id="btnQTY2" type="submit" value="查詢" class="btn btn-default" />
            <img id="imgLOADING2" src="~/Images/opc-ajax-loader.gif" style="display: none" />
        </div>
    </div>

}

<div id="pnlKEEPLIST" style="margin: 20px 0">

</div>