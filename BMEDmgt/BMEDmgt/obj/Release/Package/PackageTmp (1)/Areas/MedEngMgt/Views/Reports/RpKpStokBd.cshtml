            <td>
                @Html.DisplayFor(modelItem => item.CustNam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocTyp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Brand)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Up1000)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dn1000)
            </td>
        </tr>
    }

</table>
