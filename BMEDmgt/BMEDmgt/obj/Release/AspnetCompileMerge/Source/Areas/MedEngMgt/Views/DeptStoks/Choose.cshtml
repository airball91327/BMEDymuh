@*<script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>*@

@using (Ajax.BeginForm("Choose", "DeptStoks",
    new AjaxOptions {
        HttpMethod = "POST",
        LoadingElementId = "imgLOADING5",
        UpdateTargetId = "pnlDEPTSTOKLIST" }))
{
    <div class="form-horizontal">
        <div class="form-group">
            @Html.Label("", "零件名稱(關鍵字)", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBox("qtyCNAME", "", new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            @Html.Label("", "材料編號", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBox("qtySTOKID", "", new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input id="btnQTY" type="submit" value="查詢" class="btn btn-default" />
                <img id="imgLOADING5" src="~/Images/opc-ajax-loader.gif" style="display: none" />
            </div>

        </div>
    </div>
}

<div id="pnlDEPTSTOKLIST">
    @*@Html.Action("List")*@
</div>
