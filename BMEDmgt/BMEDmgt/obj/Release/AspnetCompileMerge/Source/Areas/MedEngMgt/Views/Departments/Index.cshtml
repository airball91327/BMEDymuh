@model IEnumerable<BMEDmgt.Areas.MedEngMgt.Models.Department>

@{
    ViewBag.Title = "部門列表";
}

<h2>部門列表</h2>

<p>
    @Html.ActionLink("新增", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DptId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name_C)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name_E)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Loc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastActivityDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DptId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name_C)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name_E)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Loc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastActivityDate)
        </td>
        <td>
            @Html.ActionLink("修改", "Edit", new { id=item.DptId }) |
            @Html.ActionLink("預覽", "Details", new { id=item.DptId })
        </td>
    </tr>
}

</table>
