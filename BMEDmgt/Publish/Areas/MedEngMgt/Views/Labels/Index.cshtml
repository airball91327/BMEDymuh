@model IEnumerable<BMEDmgt.Areas.MedEngMgt.Models.Label>

@{
    ViewBag.Title = "列表/標籤";
}

<h2>標籤列表</h2>

<p>
    @Html.ActionLink("新增", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LabelName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rtp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rtt)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LabelName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rtp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rtt)
        </td>
        <td>
            @Html.ActionLink("編輯", "Edit", new { id=item.LabelId }) |
            @Html.ActionLink("預覽", "Details", new { id=item.LabelId }) |
            @Html.ActionLink("刪除", "Delete", new { id=item.LabelId })
        </td>
    </tr>
}

</table>
